#
#	startup configuration for @PROJECT_NAME@ ipt node
#	created : ${${PROJECT_NAME}_NOW_UTC}
#	platform: @CMAKE_SYSTEM@
#
[service]
enabled = false
name = node_ipt_v${${PROJECT_NAME}_VERSION_MAJOR}.${${PROJECT_NAME}_VERSION_MINOR}

[setup]
json = $ENV{ProgramData}\node\ipt_v${${PROJECT_NAME}_VERSION_MAJOR}.${${PROJECT_NAME}_VERSION_MINOR}.json
pool-size = ${${PROJECT_NAME}_POOL_SIZE}
